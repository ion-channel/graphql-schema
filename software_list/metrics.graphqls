type RiskScope {
  name: String!
  value: Int
}

type Risk {
  score: Int
  scopes: [RiskScope!]!
}

type Compliance {
  passing: Int!
  failing: Int!
}

type Resolution {
  resolved: Int!
  partially_resolved: Int!
  unresolved: Int!
}

type Metrics {
  risk: Risk!
  compliance: Compliance!
  resolution: Resolution!
}
