enum ComponentStatus {
    no_resolution
    partial_resolution
    resolved
    errored
    deleted
}

type ComponentSuggestion {
    key: String!
    value: String!
}

type Component {
    id: String!
    sbom_id: String!
    name: String!
    version: String!
    org: String!
    status: ComponentStatus!
    search_results: SearchResults!
    suggestions: [ComponentSuggestion!]!
    created_at: Time!
    updated_at: Time!
    deleted_at: Time
    error_message: String
}
