enum OrganizationRole {
    Owner
    Manager
    Member
}

type OrganizationMember {
    user_id: String!
    username: String!
    role: OrganizationRole!
    created_at: Time!
    deleted_at: Time
}

type Organization {
    id: String!
    created_at: Time!
    updated_at: Time!
    deleted_at: Time
    name: String!
    members: [OrganizationMember!]!
}
